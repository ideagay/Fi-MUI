{
  "name": "financial-ui",
  "version": "0.1.0",
  "main": "lib/financial-ui.common.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name financial-ui ./src/index.js --dest lib",
    "build:demo": "vue-cli-service build",
    "build:lib": "rm -rf lib && npm run build && gulp less --gulpfile packages/theme/gulpfile.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "cheerio": "^1.0.0-rc.2",
    "cp-cli": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-less": "^4.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-plugin-autoprefix": "^2.0.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-container": "^2.0.0",
    "transliteration": "^2.1.3",
    "vmark-loader": "^1.0.0",
    "vue-loader": "^14.2.3",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
